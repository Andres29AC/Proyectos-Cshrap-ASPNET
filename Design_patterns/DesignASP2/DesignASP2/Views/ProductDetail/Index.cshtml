@{
    decimal totalLocal = (decimal)ViewBag.TotalLocal;
    decimal totalUSD = (decimal)ViewBag.TotalForeign;
}

<div class="row">
    <div class="col-md-12">
		<label>Total Local</label>
		<b>@totalLocal</b>
	</div>
</div>

<div class="row">
	<div class="col-md-12">
		<label>Total USD</label>
		<b>@totalUSD</b>
	</div>
</div>